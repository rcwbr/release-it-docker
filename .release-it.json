{
  "extends": [
    "github:rcwbr/release-it-docker/file-bumper#0.7.2"
  ],
  "plugins": {
    "@release-it/bumper": {
      "out": [
        {
          "file": "conventional-changelog/.release-it.json",
          "type": "text/plain"
        },
        {
          "file": "file-bumper/.release-it.json",
          "type": "text/plain"
        },
        {
          "file": "file-bumper/tag-only/.release-it.json",
          "type": "text/plain"
        }
      ]
    }
  }
}
