{
  "extends": [
    "github:rcwbr/release-it-docker/conventional-changelog#0.8.0"
  ],
  "git": {
    "getLatestTagFromAllRefs": true,
    "pushArgs": ["--tags"]
  },
  "plugins": {
    "@release-it/bumper": {
      "out": {
        "file": "VERSION",
        "type": "text/plain",
        "consumeWholeFile": true
      }
    }
  }
}
